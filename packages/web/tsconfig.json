{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "baseUrl": ".",
    "paths": {
      "@paperwait/core/*": ["../core/src/*"],
      "~/api/*": ["src/pages/api/_api/*"],
      "~/app/*": ["src/pages/[...app]/_app/*"],
      "~/components/*": ["src/components/*"],
      "~/layouts/*": ["src/layouts/*"],
      "~/middleware/*": ["src/middleware/*"],
      "~/pages/*": ["src/pages/*"],
      "~/styles/*": ["src/styles/*"]
    }
  }
}
