{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "baseUrl": ".",
    "paths": {
      "~/api/*": ["src/pages/_api/*"],
      "~/app/*": ["src/pages/_app/*"],
      "~/components/*": ["src/components/*"],
      "~/layouts/*": ["src/layouts/*"],
      "~/middleware/*": ["src/middleware/*"],
      "~/pages/*": ["src/pages/*"],
      "~/styles/*": ["src/styles/*"],
      "~/types/*": ["src/types/*"]
    }
  }
}
