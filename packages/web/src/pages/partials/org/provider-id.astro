---
import { fallback, parse } from "valibot";

import RegisterOrgProviderId from "~/components/org/provider-id.astro";
import { Registration } from "~/lib/schemas";

export const partial = true;

const provider = parse(
  fallback(Registration.entries.authProvider, "entra-id"),
  Astro.url.searchParams.get("authProvider"),
);
---

<RegisterOrgProviderId {provider} />
